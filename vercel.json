{
  "builds": [{
      "src": "studybud/wsgi.py",
      "use": "@vercel/python",
      "config": { "maxLambdaSize": "15mb", "runtime": "python3.9" }
  },
  {
    "src": "build_files.sh",
    "use": "@vercel/static-build",
    "config": { "disDir": "static" }
    }],
  "routes": [
      {
          "src": "/(.*)",
          "dest": "studybud/wsgi.py"
      }
  ]
}